{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "warn",
        "quotes": [
            "warn",
            "prefer-single"
        ],
        "indent" : ["warn", 4],
        "max-len": ["error", {"code": 120}]
    }
}